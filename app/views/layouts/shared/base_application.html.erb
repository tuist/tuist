<!DOCTYPE html>
<html class="bg-gray-900">
  <head>
    <title>TuistCloud</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <!-- Stripe -->
    <% if !@self_hosted %>
      <%= stripe_javascript_tag %>
    <% end %>

     <!-- ViteJS -->
    <%= vite_client_tag %>
    <%= vite_react_refresh_tag %>
    <%= yield(:react_app) %>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
